  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "autoprefixer": "^9.8.6",
    "aws-sdk": "^2.780.0",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "css-loader": "^4.1.1",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "get-blob-duration": "^1.1.2",
    "helmet": "^3.23.3",
    "mini-css-extract-plugin": "^0.9.0",
    "mongoose": "^5.9.25",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-sass": "^4.14.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "pug": "^3.0.0",
    "sass-loader": "^9.0.2",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.12",
    "yarn": "^1.22.10"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' ",
    "dev:assets": "cd src && cross-env WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && cross-env WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "xcopy \"src\\static\" \"build\\static\" && xcopy \"src\\views\" \"build\\views\" /s /h /e /d /y",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rd /s \"build\"",
    "ngrok": "ngrok http 4000",
    "start": "node build/init.js"
  },
  "devDependencies": {
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "eslint": "^7.5.0",
    "nodemon": "^2.0.4"
  }
}
